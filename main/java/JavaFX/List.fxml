<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<?import javafx.scene.text.Text?>
<GridPane
        prefHeight="93.0"
        prefWidth="235.0"
        xmlns:fx="http://javafx.com/fxml/1"
        xmlns="http://javafx.com/javafx/8.0.141">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Label prefHeight="18.0" prefWidth="113.0" text="Login:" />
      <Label prefHeight="18.0" prefWidth="113.0" text="Hasło:" GridPane.rowIndex="1"/>
      <Text  GridPane.columnIndex="1"
             text=""
             fx:id="login"/>
      <Text  GridPane.columnIndex="1" GridPane.rowIndex="1"
             fx:id="password"/>
      <Button mnemonicParsing="false" prefHeight="29.0" prefWidth="114.0" text="POKAŻ" GridPane.columnIndex="1" GridPane.rowIndex="2"
              fx:id="showButton" onAction="#show"/>
   </children>
</GridPane>
